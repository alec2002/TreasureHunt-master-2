import static org.junit.Assert.*;

import java.util.Arrays;

import org.junit.Test;


public class testStuff {
	int arr[] = {6,3,12,7,6};
	int arr2[] = {3,6,6,7,12};
	@Test
	public void test() {
		int arr[] = {6,3,12,7,6};
		int arr2[] = {3,6,6,7,12};
		assertArrayEquals(arr2, sortArray(arr));
	}
	sortArray(){
		Arrays.sort(arr);
	}

}
