
import java.util.ArrayList;
import java.util.List;


public class Algorithm {
	static int num = 0;
public static void main(String[] args) {
	Algorithm x = new Algorithm();
}
Algorithm(){
	
}
static public int findBrokenEgg(List<String> c){
	for (String e : c) {

	
		if (e.equals("Cracked")) {
			
			return num;
			
		}
		num += 1;
	}
	
	
}
}
